package tugas;
import java.util.Scanner;
public class Main {
    
    public static void main(String[] args) {
        Scanner input = new Scanner (System.in);
        
        int a,b,c,d,e;
        
        System.out.print("gaji pokok : ");
        
        a = input.nextInt();
        
        
        b = 55000;
        
        System.out.print("gaji lembur : ");
        
        
        c = input.nextInt();
        
        d = c*b;
        
        System.out.print(d);
        d = input.nextInt();
        
        e = d*a;
        
        System.out.print("jadi gaji bersihnya adalah: "+e);
        
        
    }
    
}
