package day60;

public class Day60 {

    public static void main(String[] args) {
    int [] angkaa = {8, 9, 2, 3};
    int jumlak = angkaa.length;
    int totall = 0;
    double rataa;
        for (int i = 0; i < jumlak; i++) {
            totall += angkaa[i];
            
            
        }
        rataa = totall/jumlak;
        System.out.println(rataa);
        }
    
        }
