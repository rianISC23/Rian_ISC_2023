package com.mycompany.rian78;

import java.util.Arrays;

import java.util.Collections;

public class Rian78 {

    public static void main(String[] args) {
        System.out.print("DERET ANGKA = ");
        Integer[] rian = {4, 6, 7, 1, 2, 9, 66, 42, 10};
        int maksimal = Collections.max(Arrays.asList(rian));
        int minimal = Collections.min(Arrays.asList(rian));
        for (int i = 0; i < rian.length; i++) {
            System.out.print(rian[i] + " ");

        }
        System.out.println(" ");
        System.out.println("NILAI MAKSIMAL = " + maksimal);
        System.out.println("NILAI MINIMAL = " + minimal);
    }
}
