package Java;
import java.util.Arrays;
public class Main {
    
    public static void main(String[] args) {
        
       
        int[] R = {1, 2, 6, 33, 81, 79, 80} ;
        int jumlah_index = R.length;
        int max = R[0] ;
        for(int y = 0; y < jumlah_index; y++) {
            if(R[y] > max ) {
                max = R[y];
            }
        }
        System.out.print("Max  = "+max);
        
        
    }
    
}
