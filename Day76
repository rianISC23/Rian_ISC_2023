package rianday76;

public class RIANday76 {

    public static void main(String[] args) {
        System.out.println("JADI RATA-RATANYA ADALAH ");
int [] o = {9, 2, 1, 2, 31};
double rata;
int total = 0;
int jumlah = o.length;
        for (int a = 0; a < jumlah; a++) {
            total += o[a];
            
            
        }
        rata = total/jumlah;
        System.out.println(rata);
    }
    
}
